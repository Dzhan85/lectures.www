# default.nginx

server {
    listen 80 default_server;

    root /usr/share/nginx/html;
    index index.html index.htm;

    include includes/fcgi.nginx;

    location  /example {
        try_files ../$uri ../$uri/ /index2.html;
    }

    location /static {
        alias /usr/share/nginx/html/static_example/static;
    }
}
